class Persona{
    private int edad;
    private String nombre;

    public void setEdad(int edad) {
        this.edad = edad;
    }

    public void setNombre(String nombre) {
        this.nombre = nombre;
    }

    public void setTelefono(String telefono) {
        this.telefono = telefono;
    }

    public int getEdad() {
        return edad;
    }

    public String getNombre() {
        return nombre;
    }

    public String getTelefono() {
        return telefono;
    }

    private String telefono;

}
public class Main {
    public static void main(String[] args) {

        // Crear clase Persona.
        //Crear variables las privadas edad, nombre y telefono de la clase Persona.
        //Crear gets y sets de cada propiedad.
        //Crear un objeto persona en el main.
        //Utiliza los gets y sets para darle valores a las propiedades

        Persona yoMismo = new Persona();
        yoMismo.setEdad(53);
        yoMismo.setNombre("David");
        yoMismo.setTelefono("66622XXXX");

        System.out.println(yoMismo.getEdad());
        System.out.println((yoMismo.getNombre()));
        System.out.println((yoMismo.getTelefono()));
    }

}
